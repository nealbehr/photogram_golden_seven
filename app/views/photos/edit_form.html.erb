<h1>Edit photo</h1>

<form action="http://localhost:3000/<%= @id %>/edit_complete">
  <div>
    <label for="photo_caption">Caption:</label>
    <input id="photo_caption" type="text" name="the_caption" value="<%= @specificPhoto.caption %>">
  </div>
  <div>
    <label for="photo_imge_url">Image URL:</label>
    <input id="photo_image_url" type="text" name="the_source" value="<%= @specificPhoto.source %>">
  </div>
  <div>
    <button>Edit Photo</button>
  </div>
</form>
